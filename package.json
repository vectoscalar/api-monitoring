{
  "name": "@siddharth-satija-vs/api-monitor-plugin",
  "version": "1.0.4",
  "main": "./dist/bundled/index.bundle.js",
  "module": "./dist/bundled/index.esm.bundle.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/bundled/index.bundle.js",
      "import": "./dist/bundled/index.esm.bundle.js"
    }
  },
  "scripts": {
    "build": "tsc && tsc-alias -p tsconfig.json",
    "postbuild": "esm2cjs --in dist/esm --out dist/cjs -l error && npm run bundle",
    "bundle": "npm run bundle:cjs && npm run bundle:esm",
    "bundle:cjs": "webpack --config webpack.config.js",
    "bundle:esm": "webpack --config webpack.esm.config.js",
    "test": "exit 0"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "check-disk-space": "^3.4.0",
    "fastify": "^4.28.1",
    "fastify-plugin": "^4.5.1",
    "joi": "^17.13.3",
    "mongoose": "^8.5.1",
    "queue": "^7.0.0",
    "request-ip": "^3.3.0",
    "uuid": "^10.0.0",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@alcalzone/esm2cjs": "^1.1.2",
    "@types/better-queue": "^3.8.6",
    "@types/request-ip": "^0.0.41",
    "@types/uuid": "^10.0.0",
    "tsc-alias": "^1.8.10"
  },
  "publishConfig": {
    "@siddharth-satija-vs:registry": "https://npm.pkg.github.com"
  }
}
